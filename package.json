{
  "name": "create-backend-template",
  "version": "1.1.6",
  "description": "Backend temlate for nodeJS projects",
  "main": "index.js",
  "bin": "./bin/cli.js",
  "repository": "https://github.com/Kwaysi/backend-template",
  "homepage": "https://github.com/Kwaysi/backend-template",
  "keywords": [
    "Express",
    "API",
    "template",
    "Sequelize",
    "Postgres",
    "Backend"
  ],
  "author": "Kilson David",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "dev": "nodemon src/index.ts",
    "start": "node build/index.js",
    "migrate": "yarn sequelize-cli db:migrate",
    "migrate:u": "yarn sequelize-cli db:migrate:undo",
    "migrate:u:a": "yarn sequelize-cli db:migrate:undo:all",
    "model": "yarn sequelize-cli model:generate --name",
    "migration": "yarn sequelize-cli migration:create --name",
    "deploy": "yarn migrate && yarn build && yarn start",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "build": "tsc src/index.ts --outDir build --esModuleInterop --target es5 --moduleResolution node"
  },
  "dependencies": {
    "@sentry/node": "^6.11.0",
    "@sentry/tracing": "^6.11.0",
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.0",
    "express-validator": "^6.12.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.1.25",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/sequelize": "^4.28.10",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.24.0",
    "typescript": "^4.3.5"
  }
}
